{
  "name": "saproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon server",
    "build": "node node_modules/webpack/bin/webpack.js --config webpack/webpack.base.js --mode=development",
    "build:prod": "npm run prep && node server",
    "build:server": "node node_modules/typescript/bin/tsc server/*.tsx --jsx react",
    "fix:immutable": "cp node_modules/immutable/dist/immutable.d.ts node_modules/@types/immutable && cp node_modules/immutable/dist/immutable-nonambient.d.ts node_modules/@types/immutable",
    "prep": "cp dist/* ./",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "author": "Brandon Morrison",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupTestFrameworkScriptFile": "mock-local-storage",
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules",
      "\\.(gif|jpg|png)$": "<rootDir>/client/app/empty-module.tsx"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
